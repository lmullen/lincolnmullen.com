---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Lincoln Mullen | Posts Feed</title>
 <link href="{{ site.base-url}}/posts.atom.xml" rel="self"/>
 <link href="http://lincolnmullen.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>{{ site.base-url }}/posts/atom.xml</id>
 <author>
   <name>Lincoln Mullen</name>
   <email>lincoln@lincolnmullen.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title type="html">{{ post.title | xml_escape }}</title>
   <link href="{{ site.base-url }}{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>{{ site.base-url }}{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>